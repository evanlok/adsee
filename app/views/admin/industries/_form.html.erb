<%= simple_form_for [:admin, @industry] do |f| %>
  <%= f.input :name %>
  <%= render 'admin/shared/image_form_field', f: f, name: :image, version: :small_square %>

  <%= f.submit 'Save', class: 'btn btn-primary' %>
  <%= link_to 'Cancel', admin_industries_path, class: 'btn btn-default' %>
<% end %>
